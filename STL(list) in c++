#include<iostream>
#include<list>
#include<iterator>
using namespace std;
void display(list<int> &l){
    list<int>::iterator iter;
    for(iter=l.begin();iter != l.end();iter++){
        cout<<*iter<<endl;
    }
}
int main(){
list<int> lst;
lst.push_back(1);
lst.push_back(2);
lst.push_back(3);
display(lst);

}
