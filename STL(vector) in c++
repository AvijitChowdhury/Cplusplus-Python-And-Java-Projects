#include<iostream>
#include<vector>
using namespace std;
void display(vector<int> &v){
    for(int i=0;i<v.size();i++){
        cout<<v[i]<<endl;
    }
}
int main(){
vector<int> vec1(1);
int size,element;
cout<<"enter the size of your vector: ";
cin>>size;
for(int i=0;i<size;i++){
    cout<<"enter the element of your vector: "<<endl;
    cin>>element;
    vec1.push_back(element);
    


}
display(vec1);
return 0;



}
